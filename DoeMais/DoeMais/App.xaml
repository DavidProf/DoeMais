<Application
    x:Class="DoeMais.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:DoeMais"
    StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Views/_Assets/Patterns/ButtonPattern.xaml" />
                <ResourceDictionary Source="Views/_Assets/Patterns/TextBoxPattern.xaml" />
                <ResourceDictionary Source="Views/_Assets/Patterns/CheckBoxPattern.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
